<p id="notice"><%= notice %></p>

<h2>Inventarios</h2>

<table>
  <thead>
    <tr>
      <th>Authority</th>
      <th>Serial</th>
      <th>Maneuver</th>
      <th>Service type</th>
      <th>Number</th>
      <th>Departure place</th>
      <th>Sinister place</th>
      <th>Driver name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @inventories.each do |inventory| %>
      <tr>
        <td><%= inventory.authority %></td>
        <td><%= inventory.serial %></td>
        <td><%= inventory.maneuver %></td>
        <td><%= inventory.service_type %></td>
        <td><%= inventory.number %></td>
        <td><%= inventory.departure_place %></td>
        <td><%= inventory.sinister_place %></td>
        <td><%= inventory.driver_name %></td>
        <td><%= link_to 'Show', inventory %></td>
        <td><%= link_to 'Edit', edit_inventory_path(inventory) %></td>
        <td><%= link_to 'Destroy', inventory, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Nuevo Inventario', new_inventory_path %>
